//present a 2D-array by function calling

# include <stdio.h>
# include <stdlib.h>

void print_vector(int i,int j, double array[i][j]){

    for(int i = 0; i < 2; i++){
        for(int j = 0; j < 3; j++){
            printf("%lf,  ", array[i][j]);
        }
        printf("\n");
    }

}

int main(){

    double array[2][3]  = {{2,4,6},{0,1,2}};

    print_vector(2,3,array);

    return 0;

};



